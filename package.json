{
  "name": "notes-app",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "api",
    "app"
  ],
  "main": "index.js",
  "scripts": {
    "build": "SKIP_PREFLIGHT_CHECK=true npm run build --workspace=app",
    "lint": "eslint ./api ./app",
